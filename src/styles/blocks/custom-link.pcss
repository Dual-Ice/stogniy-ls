.custom-link {
  cursor: pointer;
  display: flex;
  align-items: center;

  &:hover {
    text-decoration: underline;
  }

  &:before {
    width: 15px;
    height: 15px;
    content: "";
    background: svg-load('link.svg', fill=$href, width=100%, height=100%) center center no-repeat;
    margin-right: 20px;
    display: inline-block;
  }
}